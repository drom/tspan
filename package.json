{
  "name": "tspan",
  "version": "0.3.2",
  "description": "markup for SVG",
  "main": "lib/index.js",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "test": "eslint lib/*.js && nyc mocha test",
    "browserify": "browserify lib/index.js > build/tspan.js",
    "rollup": "rollup -f umd -n tspan -o build/tspan.js -- lib/parse.js",
    "prepublish": "npm run test && mkdir -p build && npm run browserify"
  },
  "files": [
    "build/tspan.js"
  ],
  "unpkg": "build/tspan.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/drom/tspan.git"
  },
  "author": "Aliaksei Chapyzhenka",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/drom/tspan/issues"
  },
  "homepage": "https://github.com/drom/tspan#readme",
  "devDependencies": {
    "browserify": "^16.2.2",
    "chai": "^4.1.0",
    "eslint": "^4.2.0",
    "mocha": "^3.4.2",
    "nyc": "^11.0.3",
    "onml": "^0.4.1",
    "react": "^15.6.1",
    "react-dom": "^15.6.1",
    "rollup": "^0.60.0"
  }
}
